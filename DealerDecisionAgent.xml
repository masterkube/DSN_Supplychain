<?xml version="1.0" encoding="UTF-8"?>
<Agent>
	<AgentName>DealerDecisionAgent</AgentName>
   <Compose>
        <Tell><action><actionName>InvQuantity</actionName></action><internal>true</internal></Tell>
        <Tell><action><actionName>Runrate</actionName></action><internal>true</internal></Tell>
        <Decision> 
            <Expression>InvQuantity&gt;Runrate</Expression>
			<TrueEvent>
                <Tell><action><actionName>SglOverStock</actionName></action><internal>true</internal></Tell>
            </TrueEvent>
			<FalseEvent>
                <Tell><action><actionName>SglUnderStock</actionName></action><internal>true</internal></Tell>
            </FalseEvent>
		</Decision>
        <Choice>
            <ChoiceOption>
                <Ask><action><actionName>SglOverStock</actionName></action><internal>true</internal></Ask>
                <OptionAction>
                    <Compose>
                        <Tell><action><actionName>OverStock</actionName></action></Tell>
                        <Sequence>
                            <Ask><action><actionName>RunDealerDecision</actionName></action><internal>true</internal></Ask>
                            <Ask><action><actionName>OverStock</actionName></action><internal>true</internal></Ask>
		                    <AgentCommand><AgentName>DecisionAgent</AgentName></AgentCommand>                        
                        </Sequence>
                    </Compose>
                </OptionAction>
            </ChoiceOption>
            <ChoiceOption>
                <Ask><action><actionName>SglUnderStock</actionName></action><internal>true</internal></Ask>
                <OptionAction>
                    <Compose>
                        <Tell><action><actionName>UnderStock</actionName></action></Tell>
                        <Sequence>
                            <Ask><action><actionName>RunDealerDecision</actionName></action><internal>true</internal></Ask>
                            <Ask><action><actionName>UnderStock</actionName></action><internal>true</internal></Ask>
                            <AgentCommand><AgentName>DecisionAgent</AgentName></AgentCommand>                        
                        </Sequence>
                    </Compose>
                </OptionAction>
            </ChoiceOption>
        </Choice>
    </Compose>
</Agent>